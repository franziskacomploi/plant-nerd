{{#if userInSession}}
    <h1>{{ userInSession.username }}</h1>
    
    <img src="" alt="User Photo">

    <img src="" alt="Birthday Cake Plant Icon">
    <h3>{{ userInSession.birthday }}</h3>

    <img src="" alt="Plant Icon">
    <h2>My favourite plant is:</h2>
    <h3>{{ userInSession.favPlant }}</h3>

    <img src="" alt="About User Icon">
    <h2>About me:</h2>
    <h3>{{ userInSession.description }}</h3>

    <button>
        <a href="">Edit your profile</a>
    </button>

    <button>
        <a href="">Delete your profile</a>
    </button>

    {{!-- <a href="/main">MAIN</a>
    <br>
    <a href="/private">PRIVATE</a> --}}
{{else}}
  <p>You still need to log in.</p>
{{/if}}



