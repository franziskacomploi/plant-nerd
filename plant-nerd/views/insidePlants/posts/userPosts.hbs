<h2>See all your posts here: </h2>
<div class="container">
{{#each posts}}
<div class="plant-post">
  <div>{{name}}</div>
  <div>{{location}}</div>
  <div>{{description}}</div>
  <img src={{plantImg}} />
  <div>{{foundOnDate}}</div>
  <div>{{season}}</div>
  <form method="POST" action="/deletePost/{{_id}}">
    <button type="Submit">Delete</button>
  </form>
  <a href="/editPost/{{_id}}">Edit</a>
</div>
{{/each}}
</div>